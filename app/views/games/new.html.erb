<% provide(:title, 'New Game') %>
<h1>New Game</h1>
<%= form_for(@game) do |f| %>
  <%= f.label :player_one %>
  <%= f.text_field :player_one, class: 'form-control' %>

  <%= f.label :player_two %>
  <%= f.text_field :player_two, class: 'form-control' %>

  <%= f.submit 'Create new game', class: 'btn btn-primary' %>
<% end %>
