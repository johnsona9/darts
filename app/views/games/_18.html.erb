<!-- 18 -->
<div class="row">
  <div class="col-sm-4">
    <h1><%= button_to("#{@game.player_one_18}", games_player_one_18_path(id: @game.id), method: :post, class: 'button') %></h1>
  </div>
  <div class="col-sm-1">
    <%= image_tag('one.png', class: 'image') if @game.player_one_18 == 1 %>
    <%= image_tag('two.png', class: 'image') if @game.player_one_18 == 2 %>
    <%= image_tag('three.png', class: 'image') if @game.player_one_18 > 2 %>
  </div>
  <div class="col-sm-2">
      <h1>18</h1>
  </div>
  <div class="col-sm-1">
    <%= image_tag('one.png', class: 'image') if @game.player_two_18 == 1 %>
    <%= image_tag('two.png', class: 'image') if @game.player_two_18 == 2 %>
    <%= image_tag('three.png', class: 'image') if @game.player_two_18 > 2 %>
  </div>
  <div class="col-sm-4">
    <h1><%= button_to("#{@game.player_two_18}", games_player_two_18_path(id: @game.id), method: :post, class: 'button') %></h1>
